{
    "$schema": "vscode://schemas/color-theme",
    "name": "Oceanic Depths (Light)",
    "type": "light",
    "colors": {
        // UI Elements - Light Theme Adjustments
        "list.warningForeground": "#055357",
        "list.errorForeground": "#003f56",
        "titleBar.activeBackground": "#003333",
        "titleBar.inactiveBackground": "#003333",
        "titleBar.activeForeground": "#ffffff", // Colour for the text in the active title bar
        "titleBar.inactiveForeground": "#ffffff", // Colour for text in the inactive title bar        
        "editor.background": "#FFFFFF", // White background for text areas
        "editor.foreground": "#003333", // Darkest Teal for text
        "editorCursor.foreground": "#FF9933", // Medium Orange for cursor
        "editor.selectionBackground": "#B2EBF280", // Lighter Aqua, semi-transparent for selection
        "editor.lineHighlightBackground": "#E0F2F7", // Very light blue for line highlight
        "editorLineNumber.foreground": "#367881", // Aqua/Light Blue for line numbers
        "editor.inactiveSelectionBackground": "#B2EBF250", // Transparent version for inactive selection
        "activityBar.background": "#F8F8F8", // Very light grey for activity bar
        "activityBar.foreground": "#003333", // Darkest Teal text
        "activityBarBadge.background": "#CC6600", // Dark Orange for badge
        "activityBarBadge.foreground": "#FFFFFF", // White text on badge
        "sideBar.background": "#F0F0F0", // Slightly darker light grey for sidebar
        "sideBar.foreground": "#003333", // Darkest Teal text
        "sideBarSectionHeader.background": "#B2EBF2", // Lighter Aqua for section headers
        "sideBarSectionHeader.foreground": "#003333",
        "statusBar.background": "#F8F8F8", // Very light grey for status bar
        "statusBar.foreground": "#003333", // Darkest Teal text
        "statusBar.debuggingBackground": "#CC6600", // Dark Orange for debugging status
        "statusBar.debuggingForeground": "#FFFFFF",
        "editorGroupHeader.tabsBackground": "#F0F0F0", // Darker background for tab bar
        "tab.activeBackground": "#FFFFFF", // Editor background for active tab
        "tab.inactiveBackground": "#F0F0F0", // Sidebar background for inactive tabs
        "tab.activeForeground": "#003333", // Darkest Teal text for active tab
        "tab.inactiveForeground": "#006666", // Dark Cyan/Teal for inactive tab text
        "tab.border": "#F0F0F0", // Border between tabs
        "button.background": "#FF9933", // Medium Orange for buttons
        "button.foreground": "#FFFFFF", // White text on buttons
        "button.hoverBackground": "#CC6600", // Dark Orange on hover
        "list.hoverBackground": "#B2EBF2", // Lighter Aqua for list item hover
        "list.activeSelectionBackground": "#B2EBF2", // Lighter Aqua for active list item
        "list.inactiveSelectionBackground": "#B2EBF2", // Lighter Aqua for inactive list item
        "input.background": "#F0F0F0", // Darker background for input fields
        "input.foreground": "#003333", // Darkest Teal text for input
        "input.placeholderForeground": "#21717c", // Aqua/Light Blue for placeholder
        "dropdown.background": "#F0F0F0", // Darker background for dropdowns
        "dropdown.foreground": "#003333", // Darkest Teal text for dropdowns
        "pickerGroup.foreground": "#003333", // Darkest Teal text for picker group headers
        "terminal.background": "#F8F8F8", // Very light grey for terminal background
        "terminal.foreground": "#003333", // Darkest Teal text for terminal
        "notificationCenterHeader.background": "#F0F0F0",
        "notificationCenterHeader.foreground": "#003333",
        "notificationToast.background": "#F8F8F8",
        "notificationToast.foreground": "#003333",
        "badge.background": "#FF9933",
        "badge.foreground": "#FFFFFF",
        "scrollbarSlider.background": "#21717c50", // Aqua/Light Blue, semi-transparent
        "scrollbarSlider.hoverBackground": "#21717c80", // Aqua/Light Blue, more transparent on hover
        "scrollbarSlider.activeBackground": "#21717c", // Aqua/Light Blue when active
        "editorWidget.background": "#F0F0F0",
        "editorWidget.border": "#FF9933",
        "debugToolBar.background": "#F0F0F0",
        "progressBar.background": "#FF9933",
        "panel.background": "#F0F0F0",
        "panelTitle.activeForeground": "#003333",
        "panelTitle.inactiveForeground": "#006666",
        "panelTitle.activeBorder": "#FF9933",
        "editorBracketMatch.background": "#B2EBF2",
        "editorBracketMatch.border": "#FF9933",
        "editorOverviewRuler.errorForeground": "#CC6600", // Dark Orange for errors
        "editorOverviewRuler.warningForeground": "#b46210", // Light Orange for warnings
        "editorOverviewRuler.infoForeground": "#21717c", // Aqua/Light Blue for info
        "gitDecoration.modifiedResourceForeground": "#FF9933", // Medium Orange for modified
        "gitDecoration.deletedResourceForeground": "#CC6600", // Dark Orange for deleted
        "gitDecoration.untrackedResourceForeground": "#21717c", // Aqua/Light Blue for untracked
        "gitDecoration.ignoredResourceForeground": "#607D8B", // Muted grey for ignored
        "gitDecoration.conflictingResourceForeground": "#b46210", // Light Orange for conflicting
        "extensionButton.prominentBackground": "#FF9933",
        "extensionButton.prominentHoverBackground": "#CC6600",
        "extensionButton.prominentForeground": "#FFFFFF",
        "settings.headerForeground": "#003333",
        "settings.modifiedItemIndicator": "#FF9933",
        "settings.dropdownBackground": "#F0F0F0",
        "settings.dropdownForeground": "#003333",
        "settings.textInputBackground": "#F0F0F0",
        "settings.textInputForeground": "#003333",
        "settings.checkboxBackground": "#F0F0F0",
        "settings.checkboxForeground": "#003333",
        "settings.numberInputBackground": "#F0F0F0",
        "settings.numberInputForeground": "#003333",
        "breadcrumb.foreground": "#006666",
        "breadcrumb.activeSelectionForeground": "#003333",
        "breadcrumbPicker.background": "#F0F0F0",
        "menu.background": "#F0F0F0",
        "menu.foreground": "#003333",
        "menu.selectionBackground": "#B2EBF2",
        "menu.selectionForeground": "#003333",
        "editor.findMatchBackground": "#FF993350",
        "editor.findMatchHighlightBackground": "#FF993330",
        "editor.wordHighlightBackground": "#21717c50",
        "editor.wordHighlightStrongBackground": "#21717c80",
        "editor.foldBackground": "#B2EBF2",
        "editorGutter.foldingControlForeground": "#003333",
        "editorHoverWidget.background": "#F0F0F0",
        "editorHoverWidget.border": "#FF9933",
        "peekView.border": "#FF9933",
        "peekViewEditor.background": "#F0F0F0",
        "peekViewResult.background": "#F0F0F0",
        "peekViewTitle.background": "#F0F0F0",
        // Terminal ANSI Colors (mapping to palette for consistency)
        "terminal.ansiBlack": "#0A0A0A", // Keep dark for contrast on light background
        "terminal.ansiRed": "#CC6600",
        "terminal.ansiGreen": "#004D40",
        "terminal.ansiYellow": "#FF9933",
        "terminal.ansiBlue": "#21717c",
        "terminal.ansiMagenta": "#b46210",
        "terminal.ansiCyan": "#006666",
        "terminal.ansiWhite": "#E0F2F7", // Light grey for white
        "terminal.ansiBrightBlack": "#607D8B",
        "terminal.ansiBrightRed": "#FFCC99",
        "terminal.ansiBrightGreen": "#006666",
        "terminal.ansiBrightYellow": "#b46210",
        "terminal.ansiBrightBlue": "#21717c",
        "terminal.ansiBrightMagenta": "#FF9933",
        "terminal.ansiBrightCyan": "#004D40",
        "terminal.ansiBrightWhite": "#FFFFFF", // Pure white for bright white
        "gitDecoration.modifiedResourceForeground": "#FF9800", // Colour for modified files
        "gitDecoration.deletedResourceForeground": "#f4de36", // Colour for deleted files
        "gitDecoration.untrackedResourceForeground": "#708587", // Colour for untracked files
        "gitDecoration.ignoredResourceForeground": "#9E9E9E", // Colour for ignored files
        "gitDecoration.conflictingResourceForeground": "#3f3a00" // Colour for conflicts          
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#607D8B" // Muted grey/teal for comments
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#006666" // Dark Cyan/Teal for strings
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#CC6600" // Dark Orange for keywords
            }
        },
        {
            "scope": [
                "number",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FF9933" // Medium Orange for numbers
            }
        },
        {
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#004D40" // Dark Teal for variables
            }
        },
        {
            "scope": [
                "function",
                "entity.name.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#b46210" // Light Orange for functions
            }
        },
        {
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#21717c" // Aqua/Light Blue for types/classes
            }
        },
        {
            "scope": [
                "operator",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#003333" // Darkest Teal for operators
            }
        },
        {
            "scope": [
                "punctuation",
                "delimiter"
            ],
            "settings": {
                "foreground": "#003333" // Darkest Teal for punctuation
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#CC6600" // Dark Orange for errors
            }
        }
    ]
}