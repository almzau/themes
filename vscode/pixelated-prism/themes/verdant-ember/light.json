    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#6D4C41" // Dark Brown for comments
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#4CAF50" // Medium Green for strings
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#F44336" // Red for keywords
            }
        },
        {
            "scope": [
                "number",
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#8BC34A" // Lime Green for numbers
            }
        },
        {
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#388E3C" // Darker Green for variables
            }
        },
        {
            "scope": [
                "function",
                "entity.name.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#D32F2F" // Darker Red for functions
            }
        },
        {
            "scope": [
                "entity.name.type",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#7ba87d" // Light Green for types/classes
            }
        },
        {
            "scope": [
                "operator",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#1A0000" // Very Dark Red/Brown for operators
            }
        },
        {
            "scope": [
                "punctuation",
                "delimiter"
            ],
            "settings": {
                "foreground": "#1A0000" // Very Dark Red/Brown for punctuation
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#D32F2F" // Darker Red for errors
            }
        }
    ]
}