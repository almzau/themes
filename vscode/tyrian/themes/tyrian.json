{
  "name": "Tyrian",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "activityBar.background": "#260339",
    "activityBar.dropBackground": "#38383820",
    "activityBar.foreground": "#ffffff",
    "activityBarBadge.background": "#FFFFFF",
    "activityBarBadge.foreground": "#383838",
    "editor.background": "#f8f8f8",
    "editor.findMatchBackground": "#38383820",
    "editor.findMatchHighlightBackground": "#38383830",
    "editor.findRangeHighlightBackground": "#38383820",
    "editor.foreground": "#383838",
    "editor.hoverHighlightBackground": "#38383810",
    "editor.lineHighlightBackground": "#38383808",
    "editor.rangeHighlightBackground": "#38383810",
    "editor.selectionBackground": "#38383820",
    "editor.selectionHighlightBackground": "#38383830",
    "editor.wordHighlightBackground": "#38383820",
    "editor.wordHighlightStrongBackground": "#38383830",
    "editorCursor.background": "#f8f8f8",
    "editorCursor.foreground": "#383838",
    "editorGroupHeader.tabsBackground": "#f0f0f0",
    "editorIndentGuide.activeBackground1": "#38383820",
    "editorIndentGuide.background1": "#38383818",
    "editorLineNumber.activeForeground": "#777777",
    "editorLineNumber.foreground": "#bbbbbb",
    "editorBracketHighlight.foreground1": "#757575",
    "editorBracketHighlight.foreground2": "#757575",
    "editorBracketHighlight.foreground3": "#757575",
    "editorBracketHighlight.foreground4": "#757575",
    "editorBracketHighlight.foreground5": "#757575",
    "editorBracketHighlight.foreground6": "#757575",
    "editorBracketHighlight.unexpectedBracket.foreground": "#db6165",    
    "list.activeSelectionBackground": "#3d1255",
    "list.activeSelectionForeground": "#ffffff",
    "list.hoverBackground": "#f0f0f0",
    "list.inactiveSelectionBackground": "#f0f0f0",
    "sideBar.background": "#f8f8f8",
    "sideBarSectionHeader.background": "#f0f0f0",
    "statusBar.background": "#e8e8e8",
    "statusBar.debuggingBackground": "#e8e8e8",
    "statusBar.foreground": "#040404",
    "statusBar.noFolderForeground": "#383838",
    "statusBar.noFolderBackground": "#e8e8e8",
    "tab.activeBackground": "#f8f8f8",
    "tab.border": "#38383820",
    "tab.inactiveBackground": "#f0f0f0",
    "button.background": "#3d1255", // Background colour of buttons
    "button.foreground": "#ffffff", // Text colour on buttons
    "button.hoverBackground": "#582872", // Background colour when hovered
    "button.secondaryBackground": "#bbbbbb", // Inactive button background
    "button.secondaryForeground": "#bbbbbb",  // Inactive button text color    
    "gitDecoration.addedResourceForeground": "#4CAF50", // Colour for added files
    "gitDecoration.modifiedResourceForeground": "#FF9800", // Colour for modified files
    "gitDecoration.deletedResourceForeground": "#f4de36", // Colour for deleted files
    "gitDecoration.untrackedResourceForeground": "#3d1255", // Colour for untracked files
    "gitDecoration.ignoredResourceForeground": "#9E9E9E", // Colour for ignored files
    "gitDecoration.conflictingResourceForeground": "#3f3a00" // Colour for conflicts    
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#383838"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#878787"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#446688"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "beginning.punctuation.definition.quote.markdown",
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "scope": "beginning.punctuation.definition.list.markdown",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "#999966"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#383838"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#446688"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#8f8f8f"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#383838"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#383838"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#446688"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#383838"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "meta.return-type",
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Control flow keywords",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "name": "Regular expression groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": [
        "keyword.operator.or.regexp",
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#3d1255"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#CC66CC"
      }
    },
    {
      "scope": [
        "variable",
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python",
        "meta.function-call.python"
      ],
      "settings": {
        "foreground": "#383838"
      }
    },
    {
      "scope": "entity.name.variable",
      "settings": {
        "foreground": "#383838"
      }
    },
    {
      "scope": "variable.other.php",
      "settings": {
        "foreground": "#7f44dd"
      }
    }
  ]
}